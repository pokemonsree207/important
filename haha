********************q6*******************************docker
###for docker using commit
mkdir mydocker
cd mydocker
docker run -it --name mycontainer ubuntu bash
echo "Hello Docker Commit" > /root/msg.txt
exit
docker commit mycontainer myimage:v1
docker images
docker run -it myimage:v1 bash
docker tag myimage:v1 mangaharini/myimage:v1
docker push mangaharini/myimage:v1

####for dockercompose.yml 
direct 
docker run -d -p 8010:80 nginx
docker run -d -p 8030:8080 tomee
# 1. Create new folder
mkdir myapp
cd myapp

# 2. Create index.html
echo "<h1>Server Running</h1>" > index.html

# 3. Create Dockerfile
echo "FROM nginx:alpine
COPY index.html /usr/share/nginx/html/index.html" > Dockerfile

# 4. Create docker-compose.yml
echo "services:
  server1:
    build: .
    ports:
      - '8080:80'

  server2:
    build: .
    ports:
      - '9090:80'
" > docker-compose.yml

# 5. Build and run BOTH servers
docker-compose up -d

# 6. Check running containers
docker ps

# 7. Open in browser
# http://localhost:8080
# http://localhost:9090


######using dockerfile
mkdir C:\Users\Dell\myapp5
cd C:\Users\Dell\myapp5
echo hello from docker harini > index.html
echo FROM nginx:latest > Dockerfile
echo COPY ./index.html /usr/share/nginx/html >> Dockerfile
echo EXPOSE 80 >> Dockerfile
docker build -t mynginx:1.0 .
docker run -d -p 9090:80 --name mynginxcontainer mynginx:1.0

